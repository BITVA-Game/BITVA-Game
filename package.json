{
  "homepage": "./",
  "name": "bitva-game",
  "version": "0.1.0",
  "private": true,
  "main": "main.js",
  "dependencies": {
    "body-parser": "^1.19.0",
    "electron": "^7.1.4",
    "express": "^4.17.1",
    "faker": "^4.1.0",
    "google-spreadsheet-to-json": "^1.0.0",
    "keygenerator": "^1.0.4",
    "prop-types": "^15.6.2",
    "react": "^16.9.0",
    "react-dev-utils": "^9.0.4",
    "react-dom": "^16.9.0",
    "react-scripts": "^3.1.2",
    "react-transition-group": "^4.3.0",
    "readline-sync": "^1.4.10",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.0",
    "superagent": "^5.1.0",
    "uifx": "^2.0.3",
    "uuid": "^3.3.3",
    "webfontloader": "^1.6.28"
  },
  "scripts": {
    "start-react": "cross-env PORT=5000 BROWSER=none react-scripts start",
    "game-server": "cross-env PORT=6000 node gameEngineServer/server.js",
    "electron": "electron .",
    "build": "react-scripts build",
    "test-react": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "start": "nf start",
    "test": "jest",
    "lint": "eslint --ext .jsx,.js . || exit 0",
    "install-win": "export npm_config_platform=win32 && npm install",
    "lintfix": "eslint --fix --ext .jsx,.js . || exit 0",
    "csslint-all": "csslint src/css/*.css",
    "csslint-all-win": "csslint .\\src\\css\\App.css .\\src\\css\\HeroSelection.css .\\src\\css\\index.css .\\src\\css\\MainMenu.css .\\src\\css\\StartScreen.css .\\src\\css\\VersusScreen.css",
    "csslint": "csslint",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=src/icons/mac/logo.icns --prune=true --out=releases",
    "package-win": "electron-packager . --overwrite --asar=true --platform=win32 --arch=x64 --icon=src/icons/win/logo.ico --prune=true --out=releases --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName='Bitva'",
    "package-linux": "electron-packager . --overwrite --asar=true --platform=linux --arch=x64 --icon=src/icons/png/1024x1024.png --prune=true --out=releases"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "@babel/preset-react": "^7.0.0",
    "@testing-library/jest-dom": "^4.0.1",
    "@testing-library/react": "^9.1.3",
    "cross-env": "^6.0.0",
    "csslint": "^1.0.5",
    "electron-packager": "^12.1.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "foreman": "^3.0.1",
    "jsdoc": "^3.6.3",
    "nock": "^11.3.5",
    "supertest": "^4.0.2",
    "url-parse": "^1.4.7",
    "wait-on": "^3.3.0"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less|sass|scss)$": "<rootDir>/__mocks__/styleMock.js",
      "\\.(png|jpg|gif|ttf|eot|svg)$": "<rootDir>/__mocks__/fileMock.js"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
